<%= form_for(@question) do |f| %>
  <% if @question.errors.any? %>
    <%= render partial: "partials/errors", locals: {object: @question} %>
  <% end %>

  <div>
    <%= f.label :question %><br>
    <%= f.text_area :question %>
  </div>
  <div>
    <%= f.check_box :active %>
    <%= f.label :active, id:"active" %><br>
  </div>
  <br>
  <div>
    <%= button_to "Submit", class: "btn waves-effect waves-light", name: "competency_id", value: @competency_id do %>
      Submit <i class="material-icons right">send</i>
    <% end %>
  </div>

<% end %>
